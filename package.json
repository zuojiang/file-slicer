{
  "name": "file-slicer",
  "version": "1.1.0",
  "description": "Thin slice upload",
  "main": "./lib/node/index.js",
  "browser": "./lib/browser/index.js",
  "scripts": {
    "start": "node test/server.js",
    "build": "npm-run-all build:*",
    "build:lib": "babel -d lib --ignore src/cli.es src",
    "build:cli": "babel -o bin/cli.js src/cli.es",
    "mocha": "mocha --exit test/index.js",
    "test:lib": "babel -w -d lib --ignore src/cli.es src",
    "test:client": "webpack -w --entry ./lib/browser/index.js -o ./test/public/bundle.js --mode development --output-library-target umd",
    "test:server": "babel-watch test/server.js",
    "test": "npm-run-all -p test:*"
  },
  "bin": {
    "file-slicer": "bin/cli.js"
  },
  "author": "zuojiang.wang@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.8",
    "execa": "^1.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "serve-static": "^1.13.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "busboy": "^0.2.14",
    "camelcase-keys": "^5.0.0",
    "cli-color": "^1.4.0",
    "connect": "^3.6.6",
    "form-data": "^2.3.3",
    "isomorphic-fetch-improve": "^1.2.0",
    "js-base64": "^2.5.0",
    "log-update": "^2.3.0",
    "mkdirp-promise": "^5.0.1",
    "pad-left": "^2.1.0",
    "pretty-bytes": "^5.1.0",
    "pretty-hrtime": "^1.0.3",
    "range-parser": "^1.2.0",
    "then-fs": "^2.0.0",
    "tsscmp": "^1.0.6",
    "uuid": "^3.3.2",
    "yargs": "^12.0.5"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuojiang/file-slicer.git"
  },
  "keywords": [
    "file",
    "slice",
    "upload",
    "thin",
    "postFile",
    "FormData",
    "folder"
  ],
  "bugs": {
    "url": "https://github.com/zuojiang/file-slicer/issues"
  },
  "homepage": "https://github.com/zuojiang/file-slicer#readme"
}
